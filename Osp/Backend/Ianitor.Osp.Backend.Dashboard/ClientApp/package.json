    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node environments/version.ts",
    "apollo:generate": "apollo client:codegen --excludes=node_modules/* --includes=projects/dashboard/src/app/graphQL/**/*.graphql --localSchemaFile=../schema.graphql --target typescript --tagName gql --outputFlat projects/dashboard/src/app/graphQL --passthroughCustomScalars --customScalarsPrefix Osp"
  },
